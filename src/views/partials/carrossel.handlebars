<!-- Carrossel -->
<section id="galeria" class="container my-5 pt-5">
  <div id="carouselLuxo" class="carousel slide animate" data-bs-ride="carousel">
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img
          src="https://picsum.photos/1200/500?1"
          class="d-block w-100"
          alt="Imagem 1"
        />
      </div>
      <div class="carousel-item">
        <img
          src="https://picsum.photos/1200/500?2"
          class="d-block w-100"
          alt="Imagem 2"
        />
      </div>
      <div class="carousel-item">
        <img
          src="https://picsum.photos/1200/500?3"
          class="d-block w-100"
          alt="Imagem 3"
        />
      </div>
    </div>
    <button
      class="carousel-control-prev"
      data-bs-target="#carouselLuxo"
      data-bs-slide="prev"
    >
      <span class="carousel-control-prev-icon"></span>
    </button>
    <button
      class="carousel-control-next"
      data-bs-target="#carouselLuxo"
      data-bs-slide="next"
    >
      <span class="carousel-control-next-icon"></span>
    </button>
  </div>
</section>